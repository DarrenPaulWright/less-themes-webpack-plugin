{
	"name": "less-themes-webpack-plugin",
	"version": "1.3.7",
	"description": "A webpack plugin for generating multiple themed css files from less.",
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"main": "src/Plugin.js",
	"files": [
		"src"
	],
	"scripts": {
		"test": "mocha",
		"test-watch": "mocha --watch",
		"patch": "npm version patch && npm publish",
		"minor": "npm version minor && npm publish",
		"major": "npm version major && npm publish",
		"preversion": "npm test && npm run docs && git add --all && git diff --staged --quiet || git commit -m \"Updating docs\"",
		"postpublish": "git push --follow-tags",
		"deps": "ncu -u && npm install",
		"deps-commit": "npm run deps && git add --all && git diff --staged --quiet || git commit -m \"Updating dependencies\"",
		"deps-patch": "npm run deps-commit && npm run patch",
		"docs": "jsdoc2md src/Plugin.js > README.md -t node_modules/dmd-readable/docs.hbs --plugin dmd-readable"
	},
	"badges": {
		"npm": {
			"image": "https://img.shields.io/npm/v/{{name}}.svg",
			"url": "https://npmjs.com/package/{{name}}"
		},
		"deps": {
			"image": "https://david-dm.org/darrenpaulwright/{{name}}.svg",
			"url": "https://david-dm.org/darrenpaulwright/{{name}}"
		},
		"size": {
			"base": "https://packagephobia.now.sh/",
			"image": "badge?p={{name}}",
			"url": "result?p={{name}}"
		},
		"vulnerabilities": {
			"base": "https://snyk.io/test/github/DarrenPaulWright/{{name}}",
			"image": "/badge.svg?targetFile=package.json",
			"url": "?targetFile=package.json"
		},
		"license": {
			"image": "https://img.shields.io/github/license/DarrenPaulWright/{{name}}.svg",
			"url": "https://npmjs.com/package/{{name}}/LICENSE.md"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DarrenPaulWright/less-themes-webpack-plugin.git"
	},
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/less-themes-webpack-plugin/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/less-themes-webpack-plugin#readme",
	"keywords": [
		"webpack",
		"less",
		"css",
		"themes"
	],
	"dependencies": {
		"css-loader": "^3.4.0",
		"less-loader": "^5.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"mkdirp": "^0.5.1",
		"postcss-loader": "^3.0.0",
		"temp": "^0.9.1"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"dmd-readable": "^1.1.0",
		"jsdoc-to-markdown": "^5.0.3",
		"mocha": "^6.2.2"
	}
}
