{
	"name": "less-themes-webpack-plugin",
	"version": "1.3.1",
	"description": "A webpack plugin for generating multiple themed css files from less.",
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"main": "src/Plugin.js",
	"scripts": {
		"test": "mocha",
		"test-watch": "mocha --watch",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs": "jsdoc2md src/Plugin.js > README.md -t docs/docs.hbs --plugin dmd-readable -d 3"
	},
